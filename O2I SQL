AND CDPOS.TABKEY = VBFA_DEL.MANDT || VBFA_DEL.VBELV 
AND CDPOS.CHNGIND = 'U' 
AND CDPOS.TABNAME = 'VBUK' 
AND CDPOS.FNAME = 'LVSTK' 
